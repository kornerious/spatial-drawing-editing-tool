{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-unresolved": [2, {
      "ignore": [
        "^[assets]",
        "^[components]",
        "^[pages]",
        "^[services]",
        "^[styles]",
        "^[app]"
      ]
    }],
    "no-underscore-dangle": "off",
    "comma-dangle": "off"
  }
}